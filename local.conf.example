# Linux Build
LINUX_QT_VERSION=5.11.2
LINUX_QT_ROOT=/opt/Qt-${LINUX_QT_VERSION}/${LINUX_QT_VERSION}/gcc_64
LINUX_BUILD_PATH=build/linux-debug
LINUX_BUILD_TYPE=RelWidthDebInfo
LINUX_QT_INSTALLER_PATH=/opt/Qt-${LINUX_QT_VERSION}/Tools/QtInstallerFramework/3.0

# Android Build
ANDROID_BUILD_PATH=build/android-release
ANDROID_BUILD_TYPE=Release
ANDROID_NDK_ROOT=/opt/android/ndk
ANDROID_SDK_ROOT=/opt/android/sdk
ANDROID_JAVA_HOME=/opt/jdk1.8.0_131
ANDROID_TOOLCHAIN_FILE_PROVIDER=NDK
ANDROID_TOOLCHAIN=clang
ANDROID_ABI=armeabi-v7a
ANDROID_STL=c++_static
ANDROID_API_LEVEL=android-28
ANDROID_QT_ROOT=/opt/Qt-${LINUX_QT_VERSION}/${LINUX_QT_VERSION}/android_armv7
ANDROID_DEPLOY_QT_KEYSTOREPATH="/path/to/keyfile.jks"
ANDROID_DEPLOY_QT_CERTIFICATEALIAS="apk-release"
ANDROID_DEPLOY_QT_STOREPASS="*****"
ANDROID_DEPLOY_QT_KEYPASS="*****"

# Win32
WIN32_QT_ROOT=c:\Qt\5.9.1\mingw53_32
WIN32_QT_INSTALLER_PATH=c:\Qt\Tools\QtInstallerFramework\3.0
WIN32_BUILD_PATH=build\win32-release
WIN32_MINGW_PATH=c:\Qt\Tools\mingw530_32

# MacOS
MACOS_QT_ROOT=/users/user/Apps/Qt/5.9.2/clang_64
MACOS_BUILD_PATH=build/macos-release
MACOS_QT_INSTALLER_PATH=/users/user/Apps/Qt/Tools/QtInstallerFramework/3.0

# iOS
IOS_QT_ROOT=/users/user/Apps/Qt/5.9.2/ios
IOS_BUILD_PATH=build/ios-release
